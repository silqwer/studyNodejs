<!DOCTYPE html>
<html>
<head>
<title>List Page</title>
</head>
<body>
	<h1>List Page</h1>
	<a href="/insert">INSERT DATA</a>
	<hr>
	<table border="1" style="width: 100%;">
		<tr>
			<th>bno</th>
			<th>subject</th>
			<th>writer</th>
			<th>regDate</th>
			<th>hit</th>
		</tr>
		<% data.forEach(function(item, index){ %>
		<tr>
			<td><%= item.bno %></td>
			<td><a href="/read/<%= item.bno %>"><%= item.subject %></a></td>
			<td><%= item.writer %></td>
			<td><%= item.regDate %></td>
			<td><%= item.hit %></td>
		</tr>
		<% });%>
		
		
		
		
		
		
		
		
	</table>
</body>
</html>